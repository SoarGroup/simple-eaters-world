sp {hierarchical-reward*propose*move
   (state <s> ^name hierarchical-reward
              ^io.input-link <il>)
   (<il> ^{ << north south east west >> <dir> } <map-object>
         ^x <x>
         ^y <y>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name move
         ^dir <dir>
         ^map-object <map-object>)
}

sp {hierarchical-reward*select*move*operator
   (state <s> ^name hierarchical-reward
              ^operator <o1> +
              ^operator <o2> +
              ^reward.<map-object1> <r1>
              ^reward.<map-object2> {<r2> < <r1>})
   (<o1> ^map-object <map-object1>)
   (<o2> ^map-object <map-object2>)
-->
   (<s> ^operator <o1> > <o2>)
}