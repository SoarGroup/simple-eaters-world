sp {hierarchical-SMEM*move*propose*forward
   (state <s> ^name move
              ^move-direction <dir>
              ^io.input-link <il>)
   (<il> ^orientation <dir>
         ^front <map-object>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name forward
         ^map-object <map-object>)
}

sp {hierarchical-SMEM*move*apply*forward
   (state <s> ^operator <op>
              ^io.output-link <ol>
              ^superstate <ss>)
   (<op> ^name forward
         ^map-object <map-object>)
-->
   (<ol> ^forward <f>)
   (<ss> ^last-move <map-object>)
}