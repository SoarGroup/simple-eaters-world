sp {move*propose*rotate
   (state <s> ^name move
              ^move-direction <> <dir>
              ^orientation <dir>)
-->
   (<s> ^operator <op> +)
   (<op> ^name rotate)
}

sp {apply*rotate
   (state <s> ^operator <op>
              ^io.output-link <out>)
  -(<out> ^rotate)
   (<op> ^name rotate)
-->
   (<out> ^rotate <r>)
}
