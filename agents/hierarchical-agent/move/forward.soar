sp {move*propose*forward
   (state <s> ^name move
              ^move-direction <dir>
              ^orientation <dir>)
-->
   (<s> ^operator <op> + =)
   (<op> ^name forward)
}

sp {apply*forward
   (state <s> ^operator <op>
              ^io.output-link <out>)
  -(<out> ^forward)
   (<op> ^name forward)
-->
   (<out> ^forward <f>)
}